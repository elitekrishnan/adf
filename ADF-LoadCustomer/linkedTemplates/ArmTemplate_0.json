		{
			"name": "[concat(parameters('factoryName'), '/203_PL_adequateshop_DataLoad')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Get Token from Web",
						"type": "WebActivity",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"url": "http://restapi.adequateshop.com/api/authaccount/login",
							"method": "POST",
							"headers": {},
							"body": {
								"value": "@concat('{\"email\":\"',pipeline().parameters.PRM_email,'\",\"password\":\"',pipeline().parameters.PRM_password,'\"}')",
								"type": "Expression"
							}
						}
					},
					{
						"name": "Token from JSON Output",
						"type": "SetVariable",
						"dependsOn": [
							{
								"activity": "Get Token from Web",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"variableName": "VAR_Token",
							"value": {
								"value": "@{activity('Get Token from Web').output.data.Token}",
								"type": "Expression"
							}
						}
					},
					{
						"name": "Get All User with Token",
						"type": "WebActivity",
						"dependsOn": [
							{
								"activity": "Token from JSON Output",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"url": "http://restapi.adequateshop.com/api/users?page=1",
							"method": "GET",
							"headers": {
								"Authorization": {
									"value": "@{concat('Bearer ',variables('VAR_Token'))}",
									"type": "Expression"
								}
							},
							"body": ""
						}
					},
					{
						"name": "ForEach User Data",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "Get All User with Token",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('Get All User with Token').output.data",
								"type": "Expression"
							},
							"isSequential": false,
							"batchCount": 10,
							"activities": [
								{
									"name": "Get Each Rows Value",
									"type": "SetVariable",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"variableName": "VAR_UserData",
										"value": {
											"value": "@{item()}",
											"type": "Expression"
										}
									}
								},
								{
									"name": "Save value to Table",
									"type": "SqlServerStoredProcedure",
									"dependsOn": [
										{
											"activity": "Get Each Rows Value",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"storedProcedureName": "[[dbo].[sp_adequateshop_UserInsert]",
										"storedProcedureParameters": {
											"JsonData": {
												"value": {
													"value": "@{variables('VAR_UserData')}",
													"type": "Expression"
												},
												"type": "String"
											},
											"pageId": {
												"value": "1",
												"type": "Int64"
											}
										}
									},
									"linkedServiceName": {
										"referenceName": "LS_SQL_ADFCustomer",
										"type": "LinkedServiceReference"
									}
								}
							]
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"PRM_email": {
						"type": "string",
						"defaultValue": "krishnanssampleapi@gmail.com"
					},
					"PRM_password": {
						"type": "string",
						"defaultValue": "123456"
					}
				},
				"variables": {
					"VAR_Token": {
						"type": "String"
					},
					"VAR_UserData": {
						"type": "String"
					}
				},
				"folder": {
					"name": "ADF Pipeline/200_PL_API_Load"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS_SQL_ADFCustomer')]"
			]
		},
